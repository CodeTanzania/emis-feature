# environment
NODE_ENV=development
PORT=5000

# request logger(morgan)
LOG_ENABLED=true
LOG_FORMAT=combined


# static serve
SERVE_STATIC=true
SERVE_STATIC_PATH=public


# body parser
BODY_PARSER_LIMIT=2mb
BODY_PARSER_JSON_TYPE=application/json

# api version
API_VERSION=1.0.0

# helmet
HELMET_HSTS=false

# mquery
MQUERY_LIMIT=10
MQUERY_MAX_LIMIT=50

# locales
DEFAULT_LOCALE=en
LOCALES=en,sw


# Feature

# Feature Admin Levels
# See: https://wiki.openstreetmap.org/wiki/Tag:boundary%3Dadministrative
FEATURE_ADMIN_LEVEL_3=Zone
FEATURE_ADMIN_LEVEL_4=Region
FEATURE_ADMIN_LEVEL_5=District
FEATURE_ADMIN_LEVEL_6=Division
FEATURE_ADMIN_LEVEL_7=Ward
FEATURE_ADMIN_LEVEL_8=
FEATURE_ADMIN_LEVEL_9=Village,Sub-ward,Mtaa
FEATURE_ADMIN_LEVEL_10=Shina

# Feature Categories
DEFAULT_FEATURE_CATEGORY=Unknown
FEATURE_CATEGORIES=Aerialway,Aeroway,Amenity,Barrier,Boundary,Building,Craft,Emergency,Geological,Highway,Historic,Landuse,Leisure,Man made,Military,Natural,Office,Place,Power,Public Transport,Railway,Route,Shop,Sport,Tourism,Waterway

# Feature Types
DEFAULT_FEATURE_TYPE=Unknown
FEATURE_TYPES=Roads,Hospital,School,Residential,Fire Hydrant,Transformer
